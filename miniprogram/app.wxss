/*
 * 全局设置
 */

 ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/*
* 刘海屏（自定义导航栏时使用）
*/

.ui-topBar {
  width: 100%;
  height: 50rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99999;
  background: linear-gradient(rgb(0, 0, 0), rgb(0, 0, 0));
}

.ui-topBarWhite {
  background: linear-gradient(rgb(255, 255, 255), rgb(255, 255, 255));
}

.ui-topBarFringe {
  height: 65rpx;
}

.ui-topBarContainer {
  height: 50rpx;
}

.ui-topBarContainerFringe {
  height: 65rpx;
}

/*
* 栅格系统
*/

.ui-row {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.ui-column {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.ui-col {
  flex: 1;
}

.ui-col-1 {
  width: 8.33%;
}

.ui-col-2 {
  width: 16.66%;
}

.ui-col-3 {
  width: 25%;
}

.ui-col-4 {
  width: 33.33%;
}

.ui-col-5 {
  width: 41.66%;
}

.ui-col-6 {
  width: 50%;
}

.ui-col-7 {
  width: 58.33%;
}

.ui-col-8 {
  width: 66.66%;
}

.ui-col-9 {
  width: 75%;
}

.ui-col-10 {
  width: 83.33%;
}

.ui-col-11 {
  width: 91.66%;
}

.ui-col-12 {
  width: 100%;
}

/*
* 对齐
*/

.ui-center {
  display: flex;
  align-items: center;
  justify-content: center;
}

.ui-row-left {
  display: flex;
  justify-content: flex-start;
}

.ui-row-center {
  display: flex;
  justify-content: center;
}

.ui-row-right {
  display: flex;
  justify-content: flex-end;
}

.ui-row-between {
  display: flex;
  justify-content: space-between;
}

.ui-row-around {
  display: flex;
  justify-content: space-around;
}

.ui-col-top {
  display: flex;
  align-items: flex-start;
}

.ui-col-center {
  display: flex;
  align-items: center;
}

.ui-col-bottom {
  display: flex;
  align-items: flex-end;
}

.ui-col-baseline {
  display: flex;
  align-items: baseline;
}

.ui-col-stretch {
  display: flex;
  align-items: stretch;
}

