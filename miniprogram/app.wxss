  left: 0;
  width: 100%;
  height: 100%;
  background-size: cover;
}

.ui-image .ui-img.ui-img-center {
  background-position: center;
}

.ui-img-bg {
  background: #f5f5f5;
}

/* 
* 1px实现 
*/

.ui-border {
  position: relative;
  border: 0;
}

.ui-border-t,
.ui-border-b,
.ui-border-l,
.ui-border-r,
.ui-border-tb {
  border: 0;
}

.ui-border-t {
  background-position: left top;
  background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef));
}

.ui-border-b {
  background-position: left bottom;
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef));
}

.ui-border-t,
.ui-border-b,
.ui-border-tb {
  background-repeat: repeat-x;
  -webkit-background-size: 100% 1px;
}

.ui-border-tb {
  background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef)),
      -webkit-gradient(linear, left top, left bottom, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef));
  background-position: top, bottom;
}

.ui-border-l {
  background-position: left top;
  background-image: -webkit-gradient(linear, right top, left top, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef));
}

.ui-border-r {
  background-position: right top;
  background-image: -webkit-gradient(linear, left top, right top, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef));
}

.ui-border-l,
.ui-border-r {
  background-repeat: repeat-y;
  -webkit-background-size: 1px 100%;
}

.ui-border:after {
  content: "";
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef)),
      -webkit-gradient(linear, left top, right top, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef)),
      -webkit-gradient(linear, left top, left bottom, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef)),
      -webkit-gradient(linear, right top, left top, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef));
  -webkit-background-size: 100% 1px, 1px 100%, 100% 1px, 1px 100%;
  -webkit-box-sizing: border-box;
  background-size: 100% 1px, 1px 100%, 100% 1px, 1px 100%;
  background-repeat: no-repeat;
  background-position: top, right, bottom, left;
  padding: 1px;
  z-index: 10;
  pointer-events: none;
}

/* 模态窗 */

.modal {
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99999;
}

.modal .modal-content {
  width: 80vw;
  margin: 10vh auto;
  border-radius: 10rpx;
  background-color: white;
}

.modal .modal-content .modal-header,
.modal .modal-content .modal-footer {
  background-size: 100% 1px;
  background-repeat: repeat-x;
}

.modal .modal-content .modal-header {
  background-position: left bottom;
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef));
}

.modal .modal-content .modal-footer {
  background-position: left top;
  background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0.5, transparent), color-stop(0.5, #efefef), to(#efefef));
}

