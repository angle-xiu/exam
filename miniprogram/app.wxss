/*
* 头部、底部栏
*/

.ui-header,
.ui-footer {
  position: fixed;
  left: 0;
  z-index: 100;
  width: 100%;
}

.ui-header {
  top: 0;
}

.ui-footer {
  bottom: 0;
}

/*
* 占位区域（无内容）
*/

.ui-placeholder-white {
  width: 100%;
  height: 20rpx;
  background-color: #fff;
}

.ui-placeholder-light {
  width: 100%;
  height: 20rpx;
  background-color: #f8f8f8;
}

/*
* 占位区域（有内容）
*/

.ui-placeholder-white-context,
.ui-placeholder-white-context-lr,
.ui-placeholder-light-context,
.ui-placeholder-light-context-lr {
  font-size: 24rpx;
  padding: 32rpx 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.ui-placeholder-light-context,
.ui-placeholder-light-context-lr {
  color: #777;
  background-color: #f8f8f8;
}

.ui-placeholder-white-context,
.ui-placeholder-white-context-lr {
  color: #bbb;
  background-color: #fff;
}

.ui-placeholder-white-context-lr::before,
.ui-placeholder-light-context-lr::before {
  content: '';
  width: 70rpx;
  height: 0;
  margin-right: 10rpx;
  border-top: 1px solid #ddd;
}

.ui-placeholder-white-context-lr::after,
.ui-placeholder-light-context-lr::after {
  content: '';
  width: 70rpx;
  height: 0;
  margin-left: 10rpx;
  border-top: 1px solid #ddd;
}

/*
* 其他常用样式
*/

.ui-visible {
  visibility: visible;
}

.ui-invisible {
  visibility: hidden;
}

.ui-hidden {
  display: none;
}

.ui-inline-block {
  display: inline-block;
}

.ui-relative {
  position: relative;
}

.ui-vertical-middle {
  vertical-align: middle;
}

.ui-nowrap {
  /* max-width: 100%;
overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis; */
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  word-break: break-all;
}

.ui-nowrap-multi {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  word-break: break-all;
}

.ui-nowrap-multi-plus {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  word-break: break-all;
}

.ui-opacity0 {
